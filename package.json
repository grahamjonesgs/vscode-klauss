{
	"name": "klauss",
	"displayName": "Klauss Assembly",
	"description": "Klauss assembly syntax support for Visual Studio Code",
	"version": "0.0.1",
	"publisher": "GrahamJones",
	"author": {
		"name": "Graham Jones"
	},
	"categories": [
		"Programming Languages"
	],
	"engines": {
		"vscode": "^0.10.1"
	},
	"galleryBanner": {
		"color": "#0084A9",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/grahamjonesgs/vscode-klauss.git"
	},
